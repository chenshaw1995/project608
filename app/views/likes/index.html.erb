
<p id="notice"><%= notice %></p>

<h1>MyMusics</h1>

<table>
  <thead>
    <tr>
      <th>Song</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if @musics.nil? then %>
     <%= "null" %>
    <%else%>
    <% @musics.each do |m| %>
      <tr>
        <!--<td><%#= Music.find(m.music_id). %></td>-->
        <!--<td><%#= m.artist %></td>-->
        <td><%= m.music_id %></td>
        <td><%= m.user_id %></td>
        <td><%#= link_to 'Destroy', method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>

<br>


