
<p id="notice"><%= notice %></p>

<h1>MyMusics</h1>

<table>
  <thead>
    <tr>
      <th>Song</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if @like_musics.nil? then %>
    <%= "  null"%>
    <%else%>
    <% @like_musics.each do |m| %>
      <tr>
        <td><%= m.name %></td>
        <td><%= m.artist %></td>
        <td><%#= link_to 'Show', music_path(like.music_id) %></td>
        <td><%#= link_to 'Edit', edit_like_path(like) %></td>
        <td><%= link_to 'Destroy', method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>

